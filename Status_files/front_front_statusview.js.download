$('.status-tabButton').on('click',function(){var tabId=$(this).attr('data-tab-id');changeTab(tabId);});function changeTab(tabId){$('.status-tabContent').filter(':visible').fadeOut(150,function(){$(this).removeClass('is-active');$('#'+tabId).fadeIn(150,function(){$(this).addClass('is-active');});});$('.status-tabButton').removeClass('is-active');$('[data-tab-id="'+tabId+'"]').addClass('is-active');};